<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="defaultNav" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">@color/navBarBackgroundColor</item>
    </style>

    <style name="defaultButton_day" parent="Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:background">@drawable/button_background_default_day</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="defaultButton_night" parent="Widget.AppCompat.Button.Borderless.Colored">
        <item name="android:background">@drawable/button_background_default_day</item>
        <item name="android:textColor">@color/color_primary_300</item>
    </style>

    <style name="StarCheckBox" parent="Theme.Packup.Activity">
        <item name="colorAccent">@color/color_vibrant_500</item>
        <item name="colorPrimary">@color/color_vibrant_500</item>
    </style>


</resources>